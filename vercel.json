{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/log",
      "methods": ["POST"],
      "dest": "/index.js"
    },
    {
      "src": "/download-logs",
      "methods": ["GET"],
      "dest": "/index.js"
    },
    {
      "src": "/download-database",
      "methods": ["GET"],
      "dest": "/index.js"
    },
    {
      "src": "/upload-database",
      "methods": ["POST"],
      "dest": "/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "engines": {
    "node": ">=14"
  }
}
